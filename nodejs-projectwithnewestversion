node
{
 
  stage("CheckOutCodeGit")
  {
   git 'https://github.com/solunwa/nodejs-application/'
 }
 
 stage("Build")
 {
 nodejs(nodeJSInstallationName: 'nodejs20.3.1') {
        sh 'npm install'
    }
 }  
 
  stage('ExecuteSonarQubeReport') {
     nodejs(nodeJSInstallationName: 'nodejs20.3.1') {
        sh 'npm run sonar'
    }
	}	
    stage('UploadintoNexus') {
       nodejs(nodeJSInstallationName: 'nodejs20.3.1') {
          sh 'npm publish'
      }
}
}
